    <item>
        <shape android:shape="rectangle">


            <size
                android:width="200dp"
                android:height="200dp"/>
        </shape>
    </item>

    <item
        android:top="100dp">
        <shape android:shape="rectangle">
            <corners android:bottomLeftRadius="6dp"
                android:bottomRightRadius="6dp"/>
            <gradient
                android:angle="270"
                android:type="linear"
                android:startColor="@color/color_Accent"
                android:centerColor="@color/white_solid"
                android:endColor="@color/lighter_white_solid"
                android:centerY=".95"
                android:centerX=".5"/>
            <size
                android:width="100dp"
                android:height="100dp"/>
        </shape>
    </item>

    <item
